<script type="text/javascript">


    function IsNumeric(dato) {
        var valor = dato.indexOf(".");
        if ((window.event.keyCode > 47 && window.event.keyCode < 58) || window.event.keyCode == 13) {
            window.event.returnValue = true;
        }
        else {
            window.event.returnValue = false;
        }
    }

    $(document).ready(function () {


        $(document).on('click', '.ver_observacion', function () {

            $('.opc_add_observacion').show();
            $('.opc_add_new_observacion').hide();

            $('#mdl_agregar_observacion_documento').modal('show');
            var id_det_documento = $(this).attr('id').split('|');

            document.getElementById('lbl_valida_observacion').innerHTML = "";

            $("#txt_id_oculto_observacion_det_documento").val(id_det_documento[1]);
            $("#tbl_lista_observacion td").remove();

            $.ajax({
                cache: false,
                type: "GET",
                url: "@(Url.RouteUrl("Listar_Observacion_x_documento_ht"))",
                data: { "id_det_documento": id_det_documento[1] },
                success: function (data) {
                    $.each(data, function (id, option) {
                        var nuevoTD = '<tr>';
                        nuevoTD += '<td>' + option.observacion + '</td>';
                        nuevoTD += '<td>' + option.nombre_usuario + '</td>';
                        nuevoTD += '</tr>';

                        jQuery("#tbl_lista_observacion").append(nuevoTD);
                    });
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    alert('Failed to retrieve states.');
                }
            });

        });


        $('#add_observacion').click(function () {
            $('.opc_add_observacion').hide();
            $('.opc_add_new_observacion').show();
            $("#txt_observacion").val("");
            document.getElementById('lbl_valida_observacion').innerHTML = "";
        });

        $('#add_cancelar_observacion').click(function () {
            $('.opc_add_observacion').show();
            $('.opc_add_new_observacion').hide();
        });

        $('#add_guardar_observacion').click(function () {
            $("#add_guardar_observacion").addClass("hidden");
            $("#add_cancelar_observacion").addClass("hidden");
            if ($("#txt_observacion").val() != "") {

                $.ajax({
                    cache: false,
                    type: "GET",
                    url: "/HojaTramite/Guardar_Observacion_x_det_documento",
                    data: {
                        "id_det_documento": $("#txt_id_oculto_observacion_det_documento").val(),
                        "observacion": $("#txt_observacion").val()
                    },
                    success: function (result) {
                        var nuevoTD = '<tr>';
                        nuevoTD += '<td>' + $("#txt_observacion").val() + '</td>';
                        nuevoTD += '<td>' + result.usuario_crea + '</td>';
                        nuevoTD += '</tr>';
                        jQuery("#tbl_lista_observacion").append(nuevoTD);
                        $('.opc_add_observacion').show();
                        $('.opc_add_new_observacion').hide();
                    }
                });
                $("#add_guardar_observacion").removeClass("hidden");
                $("#add_cancelar_observacion").removeClass("hidden");
            }
            else {
                document.getElementById('lbl_valida_observacion').innerHTML = "Debe de ingresar la observación en el recuadro";
                $("#add_guardar_observacion").removeClass("hidden");
                $("#add_cancelar_observacion").removeClass("hidden");
            }
        });

        $(document).on('click', '.Imprimir', function () {
            var hoja_tramite = $(this).attr('id');
            var url = "/Hojatramite/Imprimir_Vacia/" + hoja_tramite;
            window.location.href = url;
        });

        $('#BTN_POR_RECIBIR').click(function () {
            var url = "/HojaTramite/Por_Recibir_Ht/";
            window.location.href = url;
        });

        $('#BTN_ENVIADOS').click(function () {
            var url = "/HojaTramite/Salida_HT/";
