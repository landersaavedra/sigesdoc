        $('.regresar').click(function () {
            var url = "javascript:history.back(1)";
            window.location.href = url;
        });

        $('#nuevoForm').submit(function (e) {

            e.preventDefault();
            
            document.getElementById('lbl_valida_nombre').innerHTML = "";
            document.getElementById('lbl_valida_licencia').innerHTML = "";
            document.getElementById('lbl_valida_direccion_legal').innerHTML = "";
            document.getElementById('lbl_valida_nombre_legal').innerHTML = "";
            $('#btnGrabar').attr("disabled", "disabled");
            $('#regresar').attr("disabled", "disabled");
            $('#lbl_valida_direccion_legal').attr("hidden", "hidden");
            $('#lbl_valida_nombre_legal').attr("hidden", "hidden");

            var entra = 0;
            var f = new Date();
            var fechaActual = (f.getDate() < 10 ? "0" + f.getDate() : f.getDate()) + "/" + ((f.getMonth() + 1) < 10 ? "0" + (f.getMonth() + 1) : (f.getMonth() + 1)) + "/" + f.getFullYear();

            if ($("#txtFechainicio").val().trim() == "") {    
                $("#txtFechainicio").val(fechaActual);
            }
            if ($("#txtFechafin").val().trim() == "") {
                $("#txtFechafin").val(fechaActual);
            }

            if ($("#txt_licencia_operacion").val().trim() == "") {
                document.getElementById('lbl_valida_licencia').innerHTML = "Ingresar Licencia";
                entra = 1;
            }

            if ($("#txt_nombre").val().trim() == "") {
                document.getElementById('lbl_valida_nombre').innerHTML = "Ingresar nombre";
                entra = 1;
            }

            if ($("#cmb_lst_indicadorprotocoloespecie").val() == 3 || $("#cmb_lst_indicadorprotocoloespecie").val() == 4) {
                if ($('#tbl_protocolo_especie >tbody >tr').length == 0)//Valida que haya elementos en la tabla
                {
                    $('#myModal_especie').modal();
                    entra = 1;
                }
            }

            if ($("#txt_id_direccion_legal").val() == "") {
                $('#lbl_valida_direccion_legal').removeAttr("hidden", "hidden");
                document.getElementById('lbl_valida_direccion_legal').innerHTML = "Falta Registrar Dirección Legal, comunicarse con asistente de la Sub Dirección";
                entra = 1;
            }

            if ($("#txt_id_nombre_legal").val() == "") {
                $('#lbl_valida_nombre_legal').removeAttr("hidden", "hidden");
                document.getElementById('lbl_valida_nombre_legal').innerHTML = "Falta Registrar Persona Legal, comunicarse con asistente de la Sub Dirección";
                entra = 1;
            }
            
            if (entra == 1) {
                $('#btnGrabar').removeAttr("disabled", "disabled");
                $('#regresar').removeAttr("disabled", "disabled");
                return;
            }

            $("#txt_especie_add").val("");

            if ($("#cmb_lst_indicadorprotocoloespecie").val() == 3 || $("#cmb_lst_indicadorprotocoloespecie").val() == 4)
            {
                if ($('#tbl_protocolo_especie >tbody >tr').length >= 0)//Valida que haya elementos en la tabla
                {
                    $('#tbl_protocolo_especie tr').each(function () {
                        var val_espe = $(this).find("td").eq(0).html();
                        if (val_espe != null) {
                            if ($("#txt_especie_add").val() == "")
                            {
                                $("#txt_especie_add").val(val_espe);
                            }
                            else
                            {
                                $("#txt_especie_add").val($("#txt_especie_add").val() + "|" + val_espe);
                            }
                        }
                    });
                }
            }


            var ind_ca = $('#IND_CONCHA_ABANICO').prop('checked');
            if (ind_ca == true) { ind_ca = "1"; } else { ind_ca = "0"; }
            var ind_otro = $('#IND_OTROS').prop('checked');
            if (ind_otro == true) { ind_otro = "1"; } else { ind_otro = "0"; }
            var ind_pec = $('#IND_PECES').prop('checked');
            if (ind_pec == true) { ind_pec = "1"; } else { ind_pec = "0"; }
            var ind_cru = $('#IND_CRUSTACEOS').prop('checked');
            if (ind_cru == true) { ind_cru = "1"; } else { ind_cru = "0"; }

            $("#txt_hid_conch_abanico").val(ind_ca);
            $("#txt_hid_otros").val(ind_otro);
            $("#txt_hid_peces").val(ind_pec);
            $("#txt_hid_crusta").val(ind_cru);
            $("#txt_fecha_inicio").val($("#txtFechainicio").val());
            $("#txt_fecha_fin").val($("#txtFechafin").val());
            
            $.ajax({
                url: this.action,
                type: this.method,
                data: new FormData(this),
                processData: false,
                contentType: false,
                beforeSend: function () {
                    $('#loaderImage').removeClass("hidden").addClass("show");
                    $('#btnGrabar').attr("disabled", "disabled");
                },
                complete: function () {
                    $('#loaderImage').removeClass("show").addClass("hidden");
                    $('#btnGrabar').removeAttr("disabled", "disabled");
                },
                success: function (result) {

                    $('.field-validation-error').each(function () {
                        $(this).removeClass("field-validation-error").addClass("field-validation-valid");
                        $(this).html('');
                    });

                    $("#conformidad").html(result)
                    $('#mdlConformidad').modal();
                },

                error: function (result) {
                    if (result.status == 500) {
                        $("#mensajes").html(result.responseText).hide();
                        $("html, body").animate({ scrollTop: 0 }, '500', 'swing', function () {
                            $("#mensajes").slideDown(500, function () { });
                        });
                    }
                    else if (result.status == 400) {
                        $('.field-validation-error').each(function () {
                            $(this).removeClass("field-validation-error").addClass("field-validation-valid");
                            $(this).html('');
                        });
                        $.each(result.responseJSON.errors, function (key, value) {
                            if (value != null) {
                                var container = $('span[data-valmsg-for="' + key + '"]');
                                container.removeClass("field-validation-valid").addClass("field-validation-error");
                                container.html(value);
                            }
                        });
                        $("#mensajes").html(result.responseJSON.summary).hide();
                        if (result.responseJSON.summary != "") {
                            $("html, body").animate({ scrollTop: 0 }, '500', 'swing', function () {
                                $("#mensajes").slideDown(500, function () { });
                            });
                        }
                    }
                }
            });


        });


    });
</script>