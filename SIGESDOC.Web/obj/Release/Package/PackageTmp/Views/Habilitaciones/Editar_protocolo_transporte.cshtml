@model SIGESDOC.Web.Models.ProtocoloViewModel

@{
    ViewBag.Title = "Nuevo Protocolo";
}

<div class="bottom hidden"></div>
<div id="mensajes"></div>

<ol class="breadcrumb" style="margin-bottom: 5px;">
    <li><a href="#">Habilitaciones</a></li>
    <li class="active">Editar Protocolo</li>
</ol>

<div class="bs-callout bs-callout-info">
    <h4>Editar Protocolo</h4>
    <p>Use el siguiente formulario para editar el protocolo</p>
    <hr />
    <h4>Expediente: @Html.Label("", (string)ViewBag.Str_Expediente) </h4>
    @if ((string)ViewBag.Str_Empresa != "")
    {
        <h4>Empresa: @Html.Label("", (string)ViewBag.Str_Empresa) </h4>
    }
    @if ((string)ViewBag.Str_Empresa == "")
    {
        <h4>Persona: @Html.Label("", (string)ViewBag.Str_Persona) </h4>
    }
    <h4>Protocolo: @Html.Label("", (string)ViewBag.var_protocolo_nombre) </h4>

</div>

<div class="modal fade" id="mdl_transporte_buscar" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title" id="myModalLabel">
                    <label id="txt_titulo_trans_add_seg">  </label>
                </h4>
            </div>
            <form method="post">
                <div class="modal-body">
                    <div class="form-horizontal">

                        <div class="form-group ">
                            <label class="col-md-3 control-label">Placa del Transporte:</label>
                            <div class="col-md-4">
                                @Html.TextBox("num_placa", null, new { @class = " form-control input-sm", @maxlength = "20", @onpaste = "return false;", @placeholder = "Placa de Transporte", @onkeypress = "return IsTransporte_vista(this.value);", @onkeyup = "return back_Transporte(this.value);" })
                            </div>
                        </div>

                        <div class="act_transporte form-group">
                            <label class="col-md-2 control-label"></label>
                            <div class="col-md-6 ">
                                @Html.DropDownList("cmb_transporte", (IEnumerable<SelectListItem>)ViewBag.lst_transporte, new { @class = "form-control input-sm" })
                            </div>
                        </div>

                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary btn-sm disabled" id="btn_Aceptar_add_transporte">Aceptar</button>
                    <button type="button" class="btn btn-default btn-sm" data-dismiss="modal">Cancelar</button>
                </div>
            </form>
        </div>
    </div>
</div>

