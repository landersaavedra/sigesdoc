@using (Html.BeginForm("Editar_protocolo_transporte", "Habilitaciones", FormMethod.Post, new { @id = "nuevoForm", @enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal">
        <ol class="breadcrumb">
            <li class="active">Información del Protocolo</li>
            @Html.TextBox("agregar_persona_legal", null, new { @class = "form-control input-sm hidden" })
            @Html.TextBox("agregar_persona_legal_DNI", null, new { @class = "form-control input-sm hidden" })
            @Html.TextBox("txt_RUC", (string)ViewBag.var_RUC, new { @class = "form-control input-sm hidden" })
            @Html.TextBox("txt_DNI", (string)ViewBag.var_DNI, new { @class = "form-control input-sm hidden" })
            @Html.TextBox("txt_id_seguimiento", (string)ViewBag.id_seguimiento, new { @class = "form-control input-sm hidden" })
            @Html.TextBox("txt_id_protocolo", (string)ViewBag.var_id_protocolo, new { @class = "form-control input-sm hidden" })
            @Html.TextBox("txt_id_dat_protocolo_transporte", (string)ViewBag.var_id_dat_pro_transporte, new { @class = "form-control input-sm hidden" })
            @Html.TextBox("txt_id_direccion_legal", (string)ViewBag.id_direccion_legal, new { @class = "form-control input-sm hidden" })
            @Html.TextBox("txt_id_nombre_legal", (string)ViewBag.id_persona_legal, new { @class = "form-control input-sm hidden" })
            @Html.TextBox("txt_id_transporte", (string)ViewBag.id_transporte, new { @class = "form-control input-sm hidden" })
            @Html.TextBox("txt_fecha_inicio", (string)ViewBag.var_fecha_ini, new { @class = "form-control input-sm hidden" })
            @Html.TextBox("txt_fecha_fin", (string)ViewBag.var_fecha_fin, new { @class = "form-control input-sm hidden" })
            @Html.TextBox("txt_especie_add", null, new { @class = "form-control input-sm hidden" })


            <div class="form-group">
                @Html.Label("Adicionar persona", new { @class = "col-md-2 control-label" })
                <div class="col-md-6">
                    @Html.TextBox("txt_persona_2", (string)ViewBag.var_txt_persona_2, new { @class = "form-control input-sm", @maxlength = "306" })
                </div>
            </div>


            <div class="form-group">
                @Html.Label("Direccion Legal", new { @class = "col-md-2 control-label" })
                <div class="col-md-6">
                    @Html.TextBox("lbl_Direccion_legal", (string)ViewBag.Str_Direccion_Legal, new { @class = "form-control input-sm", @readonly = "readonly" })
                </div>
                <label id="lbl_valida_direccion_legal" style="color: #B44D4D" hidden="hidden">  </label>
                @if ((string)ViewBag.Str_Empresa != "")
                {
                    <button id="btn_direccion_legal" type="button" class="btn btn-primary btn-sm" data-toggle="modal" data-target="#mdldireccion_legal">
                        Cambiar Dirección Legal
                    </button>
                    <button id="btn_nueva_direccion" type="button" class="btn btn-primary btn-sm" data-toggle="modal" data-target="#mdldireccion_crea">
                        Nueva Dirección
                    </button>
                }
                @if ((string)ViewBag.Str_Empresa == "")
                {
                    <button id="btn_direccion_legal" type="button" class="btn btn-primary btn-sm hidden" data-toggle="modal" data-target="#mdldireccion_legal">
                        Cambiar Dirección Legal
                    </button>
                    <button id="btn_nueva_direccion" type="button" class="btn btn-primary btn-sm hidden" data-toggle="modal" data-target="#mdldireccion_crea">
                        Nueva Dirección
                    </button>
                }
            </div>

